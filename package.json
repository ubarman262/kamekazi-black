{
  "name": "kamekazi-dark",
  "displayName": "Kamekazi Dark",
  "description": "A Visual Studio Code theme for those of us who like to code late into the night",
  "version": "1.1.1",
  "publisher": "UjjwalBarman",
  "author": {
    "name": "UJJWAL BARMAN",
    "email": "Ujjwalbarman3624@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ubarman262/kamekazi-black"
  },
  "galleryBanner": {
    "color": "#282c34",
    "theme": "dark"
  },
  "scripts": {
    "build": "ts-node src/index.ts",
    "prepackage": "if not exist build mkdir build",
    "package": "vsce package --out build/",
    "publish": "vsce publish",
    "start": "nodemon",
    "lint": "eslint src --ext ts"
  },
  "nodemonConfig": {
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "bugs": {
    "url": "https://github.com/ubarman262/kamekazi-black/issues"
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "icon": "logo.png",
  "keywords": [
    "Black theme",
    "Black theme",
    "dark theme",
    "pure dark theme",
    "pure black theme",
    "best dark theme",
    "awesome dark theme",
    "awesome black theme",
    "best black theme",
    "Black",
    "Kamekazi",
    "Theme",
    "Dark Theme",
    "Customize",
    "Professional",
    "One Theme",
    "One Dark",
    "icon",
    "theme",
    "product",
    "product-icon-theme"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Kamekazi Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/theme-color.json"
      },
      {
        "label": "Kamekazi Dark (Italic)",
        "uiTheme": "vs-dark",
        "path": "./themes/theme-color-italic.json"
      }
    ],
    "productIconThemes": [
      {
        "id": "icons-kamekazi-dark",
        "label": "Kamekazi Dark Icons",
        "path": "./properties-icon/icons-carbon.json"
      }
    ],
    "iconThemes": [
      {
        "id": "icons-kamekazi-dark",
        "label": "Kamekazi Dark Icons",
        "path": "./icons-theme/helium-icons.json",
        "_watch": true
      }
    ]
  },
  "dependencies": {
    "@types/node": "^17.0.19",
    "@typescript-eslint/eslint-plugin-tslint": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "vsce": "^2.6.7"
  }
}
